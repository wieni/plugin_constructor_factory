{
    "name": "wieni/plugin_constructor_factory",
    "type": "drupal-module",
    "description": "A Drupal module providing a plugin factory allowing you to inject dependencies using the constructor",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "drupal/core": "^8.0 || ^9.0"
    },
    "require-dev": {
        "composer-runtime-api": "^2.0",
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
